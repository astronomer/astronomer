{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "properties": {
    "astronomer": {
      "type": "object",
      "properties": {
        "houston": {
          "type": "object",
          "properties": {
            "config": {
              "type": "object",
              "properties": {
                "deployments": {
                  "type": "object",
                  "properties": {
                    "helm": {
                      "type": "object",
                      "additionalProperties": false,
                      "properties": {
                        "airflow": {
                          "allOf": [
                            {
                              "$ref": "https://raw.githubusercontent.com/apache/airflow/helm-chart/1.3.0/chart/values.schema.json"
                            },
                            {
                              "additionalProperties": false,
                              "properties": {
                                "affinity": {},
                                "airflowConfigAnnotations": {},
                                "airflowHome": {},
                                "airflowLocalSettings": {},
                                "airflowPodAnnotations": {},
                                "airflowVersion": {},
                                "allowPodLaunching": {},
                                "cleanup": {},
                                "config": {},
                                "createUserJob": {},
                                "dags": {},
                                "data": {},
                                "defaultAirflowRepository": {},
                                "defaultAirflowTag": {},
                                "elasticsearch": {},
                                "env": {},
                                "executor": {},
                                "extraConfigMaps": {},
                                "extraEnv": {},
                                "extraEnvFrom": {},
                                "extraSecrets": {},
                                "fernetKey": {},
                                "fernetKeySecretName": {},
                                "flower": {},
                                "fullnameOverride": {},
                                "gid": {},
                                "images": {},
                                "ingress": {},
                                "kerberos": {},
                                "kubeVersionOverride": {},
                                "labels": {},
                                "limits": {},
                                "logs": {},
                                "migrateDatabaseJob": {},
                                "multiNamespaceMode": {},
                                "nameOverride": {},
                                "networkPolicies": {},
                                "nodeSelector": {},
                                "pgbouncer": {},
                                "podTemplate": {},
                                "podMutation": {},
                                "ports": {},
                                "postgresql": {},
                                "quotas": {},
                                "rbac": {},
                                "redis": {},
                                "registry": {},
                                "scheduler": {},
                                "secret": {},
                                "statsd": {},
                                "tolerations": {},
                                "triggerer": {},
                                "uid": {},
                                "useAstroSecurityManager": {},
                                "webserver": {},
                                "webserverSecretKey": {},
                                "webserverSecretKeySecretName": {},
                                "workers": {}
                              }
                            }
                          ]
                        },
                        "authSidecar": {
                          "type": "object"
                        },
                        "extraObjects": {
                          "type": "array"
                        },
                        "helmValuesRevisionId": {
                          "type": "integer"
                        },
                        "ingress": {
                          "type": "object"
                        },
                        "platform": {
                          "type": "object"
                        },
                        "sccEnabled": {
                          "type": "boolean"
                        },
                        "workers": {
                          "type": "object",
                          "additionalProperties": false,
                          "properties": {
                            "autoscaling": {
                              "type": "object",
                              "additionalProperties": false,
                              "properties": {
                                "enabled": {
                                  "type": "boolean"
                                },
                                "maxReplicas": {
                                  "type": "integer"
                                },
                                "minReplicas": {
                                  "type": "integer"
                                },
                                "targetCPUUtilization": {
                                  "type": "integer"
                                },
                                "targetMemoryUtilization": {
                                  "type": "integer"
                                }
                              }
                            }
                          }
                        }
                      }
                    },
                    "loggingSidecar": {
                      "type": "object",
                      "additionalProperties": false,
                      "properties": {
                        "enabled": { "type": "boolean" },
                        "repository": { "type": "string" },
                        "tag": { "type": "string" },
                        "resources": { "type": "object" },
                        "sidecarEnv": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "additionalProperties": false,
                            "properties": {
                              "name": { "type": "string" },
                              "value": { "type": "string" }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "title": "Values",
  "type": "object"
}
