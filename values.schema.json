{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "properties": {
    "astronomer": {
      "type": "object",
      "properties": {
        "houston": {
          "type": "object",
          "properties": {
            "config": {
              "type": "object",
              "properties": {
                "deployments": {
                  "type": "object",
                  "properties": {
                    "helm": {
                      "type": "object",
                      "additionalProperties": false,
                      "properties": {
                        "airflow": {
                          "type": "object"
                        },
                        "authSidecar": {
                          "type": "object"
                        },
                        "extraObjects": {
                          "type": "array"
                        },
                        "helmValuesRevisionId": {
                          "type": "integer"
                        },
                        "ingress": {
                          "type": "object"
                        },
                        "platform": {
                          "type": "object"
                        },
                        "sccEnabled": {
                          "type": "boolean"
                        },
                        "workers": {
                          "type": "object",
                          "additionalProperties": false,
                          "properties": {
                            "affinity": {
                              "type": "object"
                            },
                            "autoscaling": {
                              "type": "object",
                              "additionalProperties": false,
                              "properties": {
                                "enabled": {
                                  "type": "boolean"
                                },
                                "maxReplicas": {
                                  "type": "integer"
                                },
                                "minReplicas": {
                                  "type": "integer"
                                },
                                "targetCPUUtilization": {
                                  "type": "integer"
                                },
                                "targetMemoryUtilization": {
                                  "type": "integer"
                                }
                              }
                            },
                            "extraContainers": {
                              "type": "array"
                            },
                            "extraInitContainers": {
                              "type": "array"
                            },
                            "extraVolumeMounts": {
                              "type": "array"
                            },
                            "extraVolumes": {
                              "type": "array"
                            },
                            "nodeSelector": {
                              "type": "object"
                            },
                            "tolerations": {
                              "type": "object"
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "title": "Values",
  "type": "object"
}
