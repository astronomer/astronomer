#!/usr/bin/env bash
set -euo pipefail

# Bit of common bash magic that sets DIR to the directory of this script
DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" >/dev/null 2>&1 && pwd )"

vers=(
 "v1.14.10"
 "v1.15.11"
 "v1.16.9"
 "v1.17.5"
 "v1.18.2"
)

for V in ${vers[@]}; do
    export KUBE_VERSION=$V
    $DIR/run-ci
done

